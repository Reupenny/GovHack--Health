{
  "name": "govhack-health",
  "version": "1.0.0",
  "description": "OpenHealth API microservices deployment with Google Cloud Run",
  "type": "module",
  "scripts": {
    "dev": "docker build -t govhack-health . && docker run -p 3000:3000 -p 3001:3001 govhack-health",
    "build": "docker build -t govhack-health .",
    "deploy": "./deploy.sh",
    "deploy:gcloud": "gcloud run deploy openhealth-services --source . --region us-central1 --allow-unauthenticated --port 8080 --min-instances 0 --max-instances 10",
    "logs": "gcloud run services logs read openhealth-services --region us-central1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reupenny/GovHack--Health.git"
  },
  "keywords": [
    "health",
    "api",
    "microservices",
    "google-cloud-run",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Reupenny/GovHack--Health/issues"
  },
  "homepage": "https://github.com/Reupenny/GovHack--Health#readme"
}
